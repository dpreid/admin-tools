#!/bin/sh
mkdir -p ../autogenerated 
cd ../autogenerated

# example: relayaccess https://dev.practable.io/access pvna 32 34 #makes files for pvna32, pvna33, and pvna34

for i in $(seq $3 $4)
do
    num=$(printf "%02d" $i)
	name="${2}${num}"
	echo "${1}/session/${name}-st-data" > "st-data.access.${name}"  
	echo "${1}/session/${name}-st-video" > "st-video.access.${name}"
done




