#!/bin/bash

#assume tokens already made by the configure script
for i in $(seq 0 ${VE_NUM})
do
	num=$(printf "%02d" $i)
	expt="test${num}"
	JUMP_HOST_TOKEN=$(<"/etc/practable/jump-${expt}.token") \
				   JUMP_HOST_LOCAL_PORT=22 \
				   JUMP_HOST_ACCESS=$(<"jump-${expt}.access") \
				   bash -c '/jump host &'
done

/session host &
sleep 1
/etc/practable/session-rules.sh






